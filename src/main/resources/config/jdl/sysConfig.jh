// 系统文件表
entity SysFileInfo {
    machineCode String,
    fileBucket String,
    fileName String,
    fileSuffix String,
    fileSizeKb Long,
    finalName String,
    filePath String,
    createTime Instant,
    createUser Long,
    updateTime Instant,
    updateUser Long
}

paginate * with pagination

// Set service options to all except few
service all with serviceImpl
